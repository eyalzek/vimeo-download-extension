{
  "name": "Vimeo Download",
  "author": "Eyal Zekaria",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Find vimeo links in active page and get download URL's for them",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action.html"
  },
  "permissions": ["tabs", "<all_urls>"]
}
